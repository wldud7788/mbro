<script type="text/javascript">
    $(function(){
        $(".korea_btn").click(function(){
            $(".dkb_english_box").hide();
            // $(".dkb_japan_box").hide();
            $(".dkb_korea_box").show();

            $(".english_btn").removeClass('active');
            // $(".japan_btn").removeClass('active');
            $(".korea_btn").addClass('active');
        });

        $(".english_btn").click(function(){
            $(".dkb_korea_box").hide();
            // $(".dkb_japan_box").hide();
            $(".dkb_english_box").show();

            // $(".japan_btn").removeClass('active');
            $(".korea_btn").removeClass('active');
            $(".english_btn").addClass('active');
        });
        // $(".japan_btn").click(function(){
        //     $(".dkb_korea_box").hide();
        //     $(".dkb_english_box").hide();
        //     $(".dkb_japan_box").show();

        //     $(".korea_btn").removeClass('active');
